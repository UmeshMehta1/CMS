<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
  <meta charset="utf-8">
  <title>Instructors</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
  <style>
    table {

      border-collapse: collapse;
      width: 100%;
    }

    th, td {
      text-align: left;
      padding: 8px;
    }

    tr:nth-child(even){background-color: #f2f2f2}

    th {
      background-color: #6699FF;
      color: white;
    }

    h1, h2{
      text-align: center;
    }

    a{
      color: black;
    }
  </style>
</head>
</head>
<body>

  <h2>Sections taught by the Instructor</h2>
  <br>
  <div class="container">
    <table>
    <tr>
      <th>Section ID</th>
      <th>Semester</th>
      <th>Year</th>
    </tr>

    <% section.forEach((sec) => {%>
    <tr>
      <td><%=sec.id%></td>
      <td><%=sec.semester%></td>
      <td><%=sec.year%></td>
    </tr>
    <%})%>

  </table>
  </div>
  

</body>
</html>
